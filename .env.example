# Firebase 설정
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# 지도 API 키 (HTTP 리퍼러 제한 적용 필요)
REACT_APP_MAPS_API_KEY=your_maps_api_key

# 날씨 API (선택적 기능)
REACT_APP_WEATHER_API_KEY=your_weather_api_key

# API 엔드포인트
REACT_APP_API_URL=https://api.mytripstyle.com

# 기본 위치 설정 (서울)
REACT_APP_DEFAULT_LOCATION_LAT=37.5642135
REACT_APP_DEFAULT_LOCATION_LNG=127.0016985
REACT_APP_DEFAULT_ZOOM=13

# 기능 플래그 (true/false)
REACT_APP_ENABLE_WEATHER=false
REACT_APP_ENABLE_MULTI_AGENT=false
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_MOCK_DATA=false

# 캐싱 및 성능 설정
REACT_APP_CACHE_TTL=86400
REACT_APP_MAX_PLACES_PER_REQUEST=20

# 오류 보고 설정
REACT_APP_ERROR_REPORTING=true
REACT_APP_ERROR_LOG_LEVEL=error
